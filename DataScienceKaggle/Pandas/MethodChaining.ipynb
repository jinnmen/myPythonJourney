def chess_index(df):
    df.index_name = 'review_id'
    return df

chess_games.pipe(chess_index)

#Correct ans:

(chess_games
    .assign(n=0)
    .groupby(['white_id', 'victory_status'])
    .n
    .apply(len)
    .reset_index()
    .pipe(lambda df: df.loc[df.white_id.isin(chess_games.white_id.value_counts().head(20).index)])
)


